{
  "expo": {
    "name": "react-native-enqura",
    "slug": "react-native-enqura",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.blupay.ortakhisse",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera for video call.",
        "NSMicrophoneUsageDescription": "This app uses the microphone for video call.",
        "NSLocationWhenInUseUsageDescription": "This app uses the location for app management log.",
        "LSApplicationQueriesSchemes": [
          "whatsapp"
        ],
        "NSFaceIDUsageDescription": "Allow ortak to use FaceID",
        "NSPhotoLibraryUsageDescription": "Allow ortak to identify you due to the regulation",
        "NFCReaderUsageDescription": "Allow ortak to use NFC",
        "UIBackgroundModes": ["audio", "voip"]
      },
      "buildNumber": "4"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.blupay.ortakhisse"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a960d1ad-3729-47bd-adcc-56f98f6cf42f"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "13.0",
            "flipper": false,
            "useFrameworks": "dynamic"
          }
        }
      ]
    ]
  }
}
